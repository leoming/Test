<?xml version="1.0" encoding="utf-8"?>
<sopconfig xmlns:syberos="http://schemas.syberos.com/rpm/res/syberos"
           syberos:sopid="com.syberos.fruit_ninija"
           syberos:versionCode="1.0.0"
           syberos:versionName="1.0.0">
    <uses-permission syberos:name="syberos.permission.ACCESS_NOTIFICATION"/>

    <application syberos:debuggable="true"
                     syberos:description="make fun"
                     syberos:icon="fruit_ninija.png"
                     syberos:name="fruit_ninija">
        <uiapp syberos:id="syberos.fruit_ninija.ui"
                   syberos:exec="bin/fruit_ninija"
                       syberos:splash="fruit_ninija.png"
                       syberos:icon="fruit_ninija.png"
                       syberos:background="continue"
                       syberos:name="fruit_ninija">
                 <url>
                    <scheme>fruit_ninija</scheme>
                 </url>
        </uiapp>
    </application>
</sopconfig>

